<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY owl11 "http://www.w3.org/2006/12/owl11#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY owl11xml "http://www.w3.org/2006/12/owl11-xml#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY ontology "http://www.massey.ac.nz/treaty/ontology#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
]>


<rdf:RDF xmlns="http://www.massey.ac.nz/treaty/ontology#"
     xml:base="http://www.massey.ac.nz/treaty/ontology"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl11="http://www.w3.org/2006/12/owl11#"
     xmlns:owl11xml="http://www.w3.org/2006/12/owl11-xml#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:ontology="http://www.massey.ac.nz/treaty/ontology#">
    <owl:Ontology rdf:about=""/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.massey.ac.nz/treaty/ontology#extends -->

    <owl:ObjectProperty rdf:about="#extends">
        <rdfs:domain rdf:resource="#Extension"/>
        <rdfs:range rdf:resource="#ExtensionPoint"/>
        <rdfs:domain rdf:resource="#JavaConcreteType"/>
        <rdfs:range rdf:resource="#JavaConcreteType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#implements -->

    <owl:ObjectProperty rdf:about="#implements">
        <rdfs:range rdf:resource="#JavaAbstractType"/>
        <rdfs:domain rdf:resource="#JavaConcreteType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#tests -->

    <owl:ObjectProperty rdf:about="#tests">
        <rdfs:range rdf:resource="#JavaAbstractType"/>
        <rdfs:domain rdf:resource="#TestSuite"/>
    </owl:ObjectProperty>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#uses -->

    <owl:ObjectProperty rdf:about="#uses">
        <rdfs:range rdf:resource="#JavaAnnotationType"/>
        <rdfs:domain rdf:resource="#JavaConcreteType"/>
    </owl:ObjectProperty>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#verfiedBy -->

    <owl:ObjectProperty rdf:about="#verfiedBy">
        <rdfs:domain rdf:resource="#JavaInstantiableClass"/>
        <rdfs:range rdf:resource="#TestSuite"/>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.massey.ac.nz/treaty/ontology#Extension -->

    <owl:Class rdf:about="#Extension">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#ExtensionPoint -->

    <owl:Class rdf:about="#ExtensionPoint">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#JavaAbstractType -->

    <owl:Class rdf:about="#JavaAbstractType">
        <rdfs:subClassOf rdf:resource="#JavaResource"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#JavaAnnotationType -->

    <owl:Class rdf:about="#JavaAnnotationType">
        <rdfs:subClassOf rdf:resource="#JavaResource"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#JavaConcreteType -->

    <owl:Class rdf:about="#JavaConcreteType">
        <rdfs:subClassOf rdf:resource="#JavaResource"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#JavaInstantiableClass -->

    <owl:Class rdf:about="#JavaInstantiableClass">
        <rdfs:subClassOf rdf:resource="#JavaConcreteType"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#JavaResource -->

    <owl:Class rdf:about="#JavaResource">
        <rdfs:subClassOf rdf:resource="#PluginResource"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#PluginResource -->

    <owl:Class rdf:about="#PluginResource">
        <rdfs:subClassOf rdf:resource="&owl;Thing"/>
    </owl:Class>
    


    <!-- http://www.massey.ac.nz/treaty/ontology#TestSuite -->

    <owl:Class rdf:about="#TestSuite">
        <rdfs:subClassOf rdf:resource="#JavaResource"/>
    </owl:Class>
    


    <!-- http://www.w3.org/2002/07/owl#Thing -->

    <owl:Class rdf:about="&owl;Thing"/>
</rdf:RDF>
