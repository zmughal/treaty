<?xml version="1.0" encoding="UTF-8"?>
<contract xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://treaty.googlecode.com/svn/tags/v1.3/schema/treaty.xsd">
	<consumer>
		<resource id="ContractSchema">
			<type>http://www.treaty.org/xml#XMLSchema</type>
			<name>treaty.xsd</name>
		</resource>
	</consumer>
	<supplier>
		<resource id="Contract">
			<type>http://www.treaty.org/xml#XMLInstance</type>
			<ref>/contract/@location</ref>
		</resource>
	</supplier>
	<constraints>
		<relationship resource1="Contract" resource2="ContractSchema" type="http://www.treaty.org/xml#instantiates"/>
	</constraints>
</contract>
