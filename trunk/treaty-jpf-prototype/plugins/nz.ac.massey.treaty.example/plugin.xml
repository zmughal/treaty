<?xml version="1.0" ?>
<!DOCTYPE plugin PUBLIC "-//JPF//Java Plug-in Manifest 1.0" "http://jpf.sourceforge.net/plugin_1_0.dtd">
<plugin id="nz.ac.massey.treaty.example" version="1.0.0" class="nz.ac.massey.treaty.example.Plugin">
