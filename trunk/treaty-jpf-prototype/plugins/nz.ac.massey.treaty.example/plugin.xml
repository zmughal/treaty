		<import plugin-id="nz.ac.massey.treaty" />
	</requires>
	<runtime>
		<library id="core" path="classes/" type="code">
			<export prefix="*" />
		</library>
	</runtime>
	<extension-point id="dateformatter">
		<parameter-def id="class" multiplicity="none-or-one"/>
		<parameter-def id="formatdef" multiplicity="none-or-one" />
		<parameter-def id="name" />
	</extension-point>

</plugin>