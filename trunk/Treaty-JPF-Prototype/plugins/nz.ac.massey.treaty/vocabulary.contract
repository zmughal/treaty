<?xml version="1.0" encoding="UTF-8"?><!-- example contract as used in the prototype --><contract xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="contract.xsd">	<extensionpoint id="vocabulary">		<resource id="R1">			<type>http://www.massey.ac.nz/se/plugincontracts/java/AbstractType</type>			<!-- this is the actual class name, it can be loaded with the plugin classloader -->			<name>nz.ac.massey.treaty.verification.ContractVocabulary</name>		</resource>	</extensionpoint>	<extension>		<resource id="R2">			<type>http://www.massey.ac.nz/se/plugincontracts/java/InstantiableClass</type>			<!-- i.e., the class name will be defined by a parameter "class" in plugin.xml -->			<ref>class</ref>		</resource>	</extension>	<constraints>		<condition resource1="R2" resource2="R1" relationship="http://www.massey.ac.nz/se/plugincontracts/java/implements"/>	</constraints></contract>