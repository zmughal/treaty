<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns="http://www.treaty.org/action/mock#"
  xml:base="http://www.treaty.org/action/mock">
  
  <owl:Ontology rdf:about=""/>   

  <owl:Class rdf:ID="NoDefaultAction">
  </owl:Class>
  <owl:Class rdf:ID="AfterAction">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A default action after verification.</rdfs:comment>
    <rdfs:label>after</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="BeforeAction">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A default action before verification.</rdfs:comment>
    <rdfs:label>before</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="OnFailureAction">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A default action on failed contract verification.</rdfs:comment>
    <rdfs:label>defaultOnFailure</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="OnSuccessAction">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A default action on succeeded contract verification.</rdfs:comment>
    <rdfs:label>defaultOnSuccess</rdfs:label>
  </owl:Class>
   <owl:Class rdf:ID="DefaultOnAllAction">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A action default in all four situations.</rdfs:comment>
    <rdfs:label>after</rdfs:label>
    <rdfs:label>before</rdfs:label>
    <rdfs:label>defaultOnFailure</rdfs:label>
    <rdfs:label>defaultOnSuccess</rdfs:label>
  </owl:Class>
 
</rdf:RDF>
