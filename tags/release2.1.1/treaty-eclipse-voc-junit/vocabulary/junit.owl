<rdf:RDF
  xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
  xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
  xmlns:owl="http://www.w3.org/2002/07/owl#"
  xmlns="http://www.treaty.org/junit#"
  xml:base="http://www.treaty.org/junit">
  
  <owl:Ontology rdf:about=""/>   
  
  <owl:Class rdf:ID="TestCase">
    <rdfs:subClassOf rdf:resource="http://www.treaty.org/java#JavaType"/>
  </owl:Class>
 
  <owl:ObjectProperty rdf:ID="verifies">
    <rdfs:range rdf:resource="#TestCase"/>
    <rdfs:domain rdf:resource="http://www.treaty.org/java#InstantiableClass"/>
  </owl:ObjectProperty>

</rdf:RDF>
