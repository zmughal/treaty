<?xml version="1.0" encoding="UTF-8"?><!-- example contract used in the prototype --><contract xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"   xsi:noNamespaceSchemaLocation="http://treaty.googlecode.com/svn/tags/v1.3/schema/treaty.xsd">  <consumer>    <resource id="TestInterface">      <type>http://www.treaty.org/java#AbstractType</type>      <name>test.net.java.treaty.mocks.TestInterface</name>    </resource>  </consumer>  <supplier>    <resource id="TestClass">      <type>http://www.treaty.org/java#InstantiableClass</type>      <ref>test/@class</ref>    </resource>  </supplier>  <constraints>    <xor>      <relationship resource1="TestClass" resource2="TestInterface" type="http://www.treaty.org/java#implements"/>    </xor>  </constraints></contract>