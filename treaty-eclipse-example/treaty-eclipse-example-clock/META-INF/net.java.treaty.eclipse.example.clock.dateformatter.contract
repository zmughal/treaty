<?xml version="1.0" encoding="UTF-8"?><!-- example contract as used in the prototype --><contract xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://treaty.googlecode.com/svn/tags/v1.0/schema/treaty.xsd">	<extensionpoint id="dateformatter">		<resource id="XP1">			<type>http://www.massey.ac.nz/se/plugincontracts/java/AbstractType</type>			<!-- this is the actual class name, it can be loaded with the plugin classloader -->			<name>net.java.treaty.eclipse.example.clock.DateFormatter</name>		</resource>		<resource id="XP2">			<type>http://www.massey.ac.nz/se/plugincontracts/junit/TestCase</type>			<name>net.java.treaty.eclipse.example.clock.DateFormatterPerformanceTests</name>		</resource>		</extensionpoint>	<extension>		<resource id="X1">			<type>http://www.massey.ac.nz/se/plugincontracts/java/InstantiableClass</type>			<ref>/serviceprovider/@class</ref>		</resource>		</extension>	<constraints>			<and>				<condition resource1="X1" resource2="XP1" relationship="http://www.massey.ac.nz/se/plugincontracts/java/implements"/>				<condition resource1="X1" resource2="XP2" relationship="http://www.massey.ac.nz/se/plugincontracts/junit/verifies"/>			</and>	</constraints></contract>