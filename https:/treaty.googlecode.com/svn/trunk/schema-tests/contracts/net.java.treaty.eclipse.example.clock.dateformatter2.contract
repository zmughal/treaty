<?xml version="1.0" encoding="UTF-8"?><!-- example contract used in the prototype --><contract xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://treaty.googlecode.com/svn/tags/v1.3/schema/treaty.xsd">	<!-- external means that this resource is neither provided by the extension -->	<!-- nor by the extension point - it is provided by THIS plugin -->	<external>		<resource id="StrictQoSTests">			<type>http://www.treaty.org/junit#TestCase</type>			<name>net.java.treaty.eclipse.example.clock.citm.DateFormatterPerformanceTests</name>		</resource>			</external>	<supplier>		<resource id="Formatter">			<type>http://www.treaty.org/java#InstantiableClass</type>			<ref>/serviceprovider/@class</ref>		</resource>		<resource id="FormatString">			<type>http://www.treaty.org/xml#XMLInstance</type>			<ref>/serviceprovider/@formatdef</ref>		</resource>		</supplier>	<constraints>		<!-- the second condition applied only if the XML schema is not provided -->		<or>			<mustExist resource="FormatString"/>			<relationship resource1="Formatter" resource2="StrictQoSTests" type="http://www.treaty.org/junit#verifies"/>		</or>	</constraints></contract>